#. Description for the -C argument
msgctxt "CWD_DESCRIPTION"
msgid "runs as if cov was started in <dir> instead of the current working directory"
msgstr "runs as if cov was started in <dir> instead of the current working directory"

#. Error message for alias, which is not properly created
msgctxt "ERROR_ALIAS"
msgid "empty alias for '{}'"
msgstr "empty alias for '{}'"

#. Error mesage for a tool, which was not recognized
msgctxt "ERROR_TOOL_NOT_RECOGNIZED"
msgid "\"{}\" is not a cov command"
msgstr "\"{}\" is not a cov command"

#. Answer for --help option for alias tool
msgctxt "HELP_ALIAS"
msgid "'{}' is aliased to '{}'"
msgstr "'{}' is aliased to '{}'"

#. Description for the add tool
msgctxt "HELP_DESCRIPTION_ADD"
msgid "appends a report to repo"
msgstr "appends a report to repo"

#. Description for the config tool
msgctxt "HELP_DESCRIPTION_CONFIG"
msgid "shows and/or sets various settings"
msgstr "shows and/or sets various settings"

#. Description for the init tool
msgctxt "HELP_DESCRIPTION_INIT"
msgid "creates a new cov repo"
msgstr "creates a new cov repo"

#. Description for the log tool
msgctxt "HELP_DESCRIPTION_LOG"
msgid "prints a list of reports"
msgstr "prints a list of reports"

#. Description for the module tool
msgctxt "HELP_DESCRIPTION_MODULE"
msgid "defines file groups"
msgstr "defines file groups"

#. Description for the remove tool
msgctxt "HELP_DESCRIPTION_REMOVE"
msgid "removes a particular report from repo"
msgstr "removes a particular report from repo"

#. Description for the serve tool
msgctxt "HELP_DESCRIPTION_SERVE"
msgid "starts a local webserver with reports"
msgstr "starts a local webserver with reports"

#. Description for the show tool
msgctxt "HELP_DESCRIPTION_SHOW"
msgid "shows a specific report"
msgstr "shows a specific report"

#. Help group title for common tools
msgctxt "HELP_GROUP_COMMON"
msgid "common commands"
msgstr "common commands"

#. Description for the --list-cmds argument
msgctxt "LIST_CMDS_DESCRIPTION"
msgid "lists known commands from requested groups"
msgstr "lists known commands from requested groups"

#. Name of a command group argument
msgctxt "SPECS_MULTI_META"
msgid "<spec>[,<spec>,...]"
msgstr "<spec>[,<spec>,...]"

#. Short usage description
msgctxt "USAGE"
msgid "[-h] [-C <dir>] <command> [<args>]"
msgstr "[-h] [-C <dir>] <command> [<args>]"

#. Description for the -v/--version argument
msgctxt "VERSION_DESCRIPTION"
msgid "shows version information and exits"
msgstr "shows version information and exits"

#. Answer for --version option
msgctxt "VERSION_IS"
msgid "cov version {}"
msgstr "cov version {}"
