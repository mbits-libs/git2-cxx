#. Description for --abbrev-hash argument
msgctxt "ABBREV_HASH_DESCRIPTION"
msgid ""
"instead of showing the full 40-byte hexadecimal report object name, shows a "
"prefix that names the objects uniquely"
msgstr ""
"instead of showing the full 40-byte hexadecimal report object name, shows a "
"prefix that names the objects uniquely"

#. Description for --format argument
msgctxt "FORMAT_DESCRIPTION"
msgid ""
"pretty-prints the contents of the report logs in a given format, where "
"<format> can be one of 'oneline', 'short', 'medium', 'full', 'fuller', "
"'reference', 'raw' and 'pretty:<string>'; when <format> is none of the "
"above, and has %placeholder in it, it acts as if --format=pretty:<format> "
"were given"
msgstr ""
"pretty-prints the contents of the report logs in a given format, where "
"<format> can be one of 'oneline', 'short', 'medium', 'full', 'fuller', "
"'reference', 'raw' and 'pretty:<string>'; when <format> is none of the "
"above, and has %placeholder in it, it acts as if --format=pretty:<format> "
"were given"

#. Name of --format argument
msgctxt "FORMAT_META"
msgid "<format>"
msgstr "<format>"

#. Description for --decorate argument
msgctxt "HOW_DESCRIPTION"
msgid ""
"prints out ref names of any reports that are shown; <how> is 'short', "
"'auto', or 'no'"
msgstr ""
"prints out ref names of any reports that are shown; <how> is 'short', "
"'auto', or 'no'"

#. Name of --decorate argument
msgctxt "HOW_META"
msgid "<how>"
msgstr "<how>"

#. Description for -n/--max-count argument
msgctxt "MAX_COUNT_DESCRIPTION"
msgid "limits the number of reports to output"
msgstr "limits the number of reports to output"

#. Description for --no-abbrev-hash argument
msgctxt "NO_ABBREV_HASH_DESCRIPTION"
msgid "shows the full 40-byte hexadecimal report object name"
msgstr "shows the full 40-byte hexadecimal report object name"

#. Name of a number argument
msgctxt "NUMBER_META"
msgid "<number>"
msgstr "<number>"

#. Description for --oneline argument
msgctxt "ONELINE_DESCRIPTION"
msgid "a shorthand for \"--format=oneline --abbrev-hash\" used together"
msgstr "a shorthand for \"--format=oneline --abbrev-hash\" used together"

#. Description for log and show target argument
msgctxt "REV_RANGE_DESCRIPTION"
msgid "shows only reports in the specified revision range"
msgstr "shows only reports in the specified revision range"

#. Name of positional argument of cov log
msgctxt "REV_RANGE_META"
msgid "<revision-range>"
msgstr "<revision-range>"

#. Description for --color argument
msgctxt "WHEN_DESCRIPTION"
msgid "uses color in output; <when> is 'never', 'always', or 'auto'"
msgstr "uses color in output; <when> is 'never', 'always', or 'auto'"

#. Name of --color argument
msgctxt "WHEN_META"
msgid "<when>"
msgstr "<when>"
