{
    "lang": "pl",
    "args": "report $DATA/build-coverage-2-files.json -f create-report",
    "patches": {
        "\u001b\\[31m\\[od\u0142\u0105czone HEAD [0-9a-fA-F]+\\]\u001b\\[m reported files": "\u001b[31m[od\u0142\u0105czone HEAD $REPORT]\u001b[m reported files",
        " \u001b\\[2mw oparciu o \u001b\\[33m[0-9a-fA-F]+@main\u001b\\[m": " \u001b[2mw oparciu o \u001b[33m$HEAD@main\u001b[m",
        " rodzic [0-9a-fA-F]+": " rodzic $PARENT"
    },
    "expected": [
        0,
        [
            "\u001b[31m[od\u0142\u0105czone HEAD $REPORT]\u001b[m reported files",
            " 2 pliki, \u001b[33m86%\u001b[m (6/7, -1)",
            " \u001b[2mw oparciu o \u001b[33m$HEAD@main\u001b[m",
            " rodzic $PARENT\n"
        ],
        [
            "[ADD] src/main.cc",
            "[ADD] src/greetings.cc",
            "cov report: ostrze\u017cenie: src/greetings.cc zosta\u0142 zmodyfikowany po raporcie\n"
        ]
    ],
    "prepare": [
        "unpack $DATA/repo.git.tar $TMP",
        "cd '$TMP'",
        "git clone repo.git",
        "cd '$TMP/repo'",
        "cov init",
        "cov report $DATA/build-coverage.json -f create-report",
        "detach $TMP/repo/.git/.covdata"
    ]
}
