{
    "env": {
        "HOME": "$TMP/config-explicit/home",
        "XDG_CONFIG_HOME": null,
        "USERPROFILE": null
    },
    "args": "config --unset-all key.name --add unimportant.item 'next value'",
    "expected": [
        2,
        "",
        [
            "usage: cov config [-h] [<file>] [<name> [<value>] | --unset <name> | -l | --list]",
            "cov config: error: argument --unset-all: not allowed with argument --add\n"
        ]
    ],
    "prepare": [
        "safe-rm $TMP/config-explicit",
        "mkdirs $TMP/config-explicit/repo.git",
        "mkdirs $TMP/config-explicit/repo.covdata",
        "cd $TMP/config-explicit/repo.covdata",
        "git init --bare $TMP/config-explicit/repo.git",
        "cov init --git-dir $TMP/config-explicit/repo.git .",
        "cd $TMP/config-explicit/repo.covdata"
    ]
}
