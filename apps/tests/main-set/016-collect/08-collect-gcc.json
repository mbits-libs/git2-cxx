{
    "args": "collect",
    "patches": {
        "\\[([^\\]]+)\\] [0-9.]+ s": "[\\1] #.## s",
        "(.+)\\.exe": "\\1"
    },
    "expected": [
        0,
        "",
        [
            "[toolkit] GNU 17.0.1 (from $TMP/collect/.covcollect)",
            "  [gcov] $TMP/mocks/gcov-17.0.1",
            "[report] start...",
            "[0/0] finished        ",
            "[report] #.## s",
            "[files] 0",
            "[lines] 0 / 0",
            "[functions] 0 / 0",
            "[i/o] $TMP/collect/build/cov-collect.json\n"
        ]
    ],
    "prepare": [
        "mkdirs '$TMP/collect'",
        "cd '$TMP/collect'",
        "git init",
        "cov init",
        "git commit -m 'initial' --allow-empty",
        "mock hal9000-lcars-GLaDOS-g++-17 g++",
        "mock hal9000-lcars-GLaDOS-gcov-17 gcov",
        "mock hal9000-lcars-GLaDOS-gcov-17 gcov-17.0.1",
        "generate '$DATA/covcollect/ini' .covcollect 'COMPILER=$TMP/mocks/g++'"
    ]
}
