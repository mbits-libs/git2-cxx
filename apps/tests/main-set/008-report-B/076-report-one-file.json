{
    "args": "report $DATA/build-coverage.json -f create-report",
    "patches": {
        "\u001b\\[31m\\[main [0-9a-fA-F]+\\]\u001b\\[m second": "\u001b[31m[main $REPORT]\u001b[m second"
    },
    "expected": [
        0,
        [
            "\u001b[31m[main $REPORT]\u001b[m second",
            " one file, \u001b[31m67%\u001b[m (2/3, -1)",
            " \u001b[2mbased on \u001b[33m384512442@main\u001b[m\n"
        ],
        "[ADD] src/main.cc\n"
    ],
    "prepare": [
        "unpack $DATA/repo.git.tar $TMP",
        "cd '$TMP'",
        "git clone repo.git",
        "cd '$TMP/repo'",
        "cov init"
    ]
}
