msgstr ""

#. Error message for removal of existing repository
msgctxt "CANNOT_REMOVE"
msgid "Cannot remove Cov repository in {0}"
msgstr ""

#. Name of the init's last argument; note this one does not have angle brackets
msgctxt "DIR_META"
msgid "directory"
msgstr ""

