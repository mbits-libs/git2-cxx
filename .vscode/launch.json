{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Launch cov-test",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/debug/bin/cov-test",
            "args": [
                "--gtest_filter=*"
            ],
            "preLaunchTask": "CMake: build (debug)"
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Launch strip-excludes",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/debug/bin/cov",
            "args": [
                "report",
                "-f",
                "strip-excludes",
                "${workspaceFolder}/apps/tests/data/empty-coverage.json",
                "-o-",
                "--",
                "-v"
            ],
            "preLaunchTask": "CMake: build (debug)"
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Launch cov collect (gdb)",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/debug/libexec/cov/cov-collect",
            "args": [],
            "preLaunchTask": "CMake: build (debug)"
        },
        {
            "type": "cppvsdbg",
            "request": "launch",
            "name": "Launch cov collect (vs)",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/debug/libexec/cov/cov-collect",
            "args": [],
            "preLaunchTask": "CMake: build (debug)"
        }
    ]
}
